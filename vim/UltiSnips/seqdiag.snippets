snippet s "seqdiag"
seqdiag {
	$0
}
endsnippet

snippet l "Label"
[label = '$1']
endsnippet

snippet == "Separator"
=== $1 ===
endsnippet

snippet -> "->"
${1:A} -> ${2:$1}${3: [label = '$4']}
endsnippet

snippet <-- "<--"
${1:A} <-- ${2:$1}${3: [label = '$4']}
endsnippet

snippet => "Relation"
${1:A} => ${2:$1}${3: [label = '$4']}
endsnippet
